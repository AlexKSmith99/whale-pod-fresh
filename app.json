{
  "expo": {
    "name": "whale-pod-fresh",
    "slug": "whale-pod-fresh",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "whalepod",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.whalepod.fresh",
      "infoPlist": {
        "NSCameraUsageDescription": "Whale Pod needs access to your camera for video calls with your pod team.",
        "NSMicrophoneUsageDescription": "Whale Pod needs access to your microphone for video calls with your pod team.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.whalepod.fresh",
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "MODIFY_AUDIO_SETTINGS"
      ],
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "85d9f8fe-e5eb-4f70-80f6-e3094a1c557d"
      }
    }
  }
}
