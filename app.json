{
  "expo": {
    "name": "whale-pod-fresh",
    "slug": "whale-pod-fresh",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.whalepod.fresh",
      "infoPlist": {
        "NSCameraUsageDescription": "Whale Pod needs access to your camera for video calls with your team.",
        "NSMicrophoneUsageDescription": "Whale Pod needs access to your microphone for video calls with your team."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
